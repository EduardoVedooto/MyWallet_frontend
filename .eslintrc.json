{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-return-assign": 0,
        "no-param-reassign": 0,
        "no-console": 0,
        "no-alert": 0,
        "no-shadow": 0,
        "import/prefer-default-export": 0,
        "react/react-in-jsx-scope": 0,
        "react/jsx-uses-react": 0,
        "react/jsx-one-expression-per-line": 0,
        "react/prop-types": 0
    }
}
